{
  "expo": {
    "name": "QuizBible",
    "slug": "QuizBible",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/quizBible-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.moreno.dev.QuizBible",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/quizBible-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.moreno.dev.QuizBible",
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [
        "com.google.android.gms.permission.AD_ID",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "com.android.vending.BILLING",
        "android.permission.RECORD_AUDIO",
        "com.google.android.gms.permission.AD_ID",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "com.android.vending.BILLING",
        "android.permission.RECORD_AUDIO"
      ],
      "googleServicesFile": "./google-services.json",
      "versionCode": 8
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-apple-authentication"
      ],
      [
        "expo-image-picker",
        {
          "cameraPermission": "Permite tomar una foto de perfil para personalizar tu cuenta.",
          "photosPermission": "Permite seleccionar una imagen desde tu galer√≠a para establecerla como foto de perfil."
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.1001847642825-mqg5h7s4n72rn3u1t9lv8osli9aamcz"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/quizBible-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-9836483267876320~2353726828",
          "iosAppId": "ca-app-pub-9836483267876320~2514822006"
        }
      ],
      [
        "expo-navigation-bar",
        {
          "position": "absolute",
          "backgroundColor": "#00000088",
          "visibility": "hidden"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/quizBible-logo.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/sound/rachasound.mp3"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "11200416-890e-443b-87b1-b16a625a5cc6"
      }
    },
    "owner": "moreno.dev",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/11200416-890e-443b-87b1-b16a625a5cc6"
    }
  }
}
