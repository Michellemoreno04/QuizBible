{
  "expo": {
    "name": "QuizBible",
    "slug": "QuizBible",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/quizBible-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.moreno.dev.QuizBible"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/quizBible-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.moreno.dev.QuizBible",
      "enableProguardInReleaseBuilds": true,
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [
       
        "com.google.android.gms.permission.AD_ID",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "usesAdvertisingId": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/quizBible-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-9836483267876320~2353726828",
          "iosAppId": "ca-app-pub-9836483267876320~2514822006"
        }
      ],
      [
        "expo-navigation-bar",
        {
          "position": "absolute",
          "backgroundColor": "#00000088",
          "visibility": "hidden"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "11200416-890e-443b-87b1-b16a625a5cc6"
      }
    },
    "owner": "moreno.dev"
  },
  "doctor": {
    "reactNativeDirectoryCheck": {
      "exclude": [
        "react-native-toast-notifications",
        "firebase"
      ]
    },
    "listUnknownPackages": false
  }
}
